<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">

        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/ds?useSSL=true" userId="root"
                        password="123456">
        </jdbcConnection>

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
            NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>


        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator targetPackage="ds.pojo"
                            targetProject=".\src">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- targetProject:mapper映射文件生成的位置 -->
        <sqlMapGenerator targetPackage="ds.mapper"
                         targetProject=".\src">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>


        <!-- targetPackage：mapper接口生成的位置 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="ds.mapper"
                             targetProject=".\src">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>


        <!-- 指定数据库表 -->
        <table schema="" tableName="ds_brands" domainObjectName="Brands">
            <generatedKey column="brand_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table schema="" tableName="ds_cart" domainObjectName="Cart">
            <generatedKey column="cart_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table schema="" tableName="ds_item_cat" domainObjectName="ItemCat">
            <generatedKey column="cat_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_item_for_items" schema="" domainObjectName="ItemForItems">
            <generatedKey column="item_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_item_pic" schema="" domainObjectName="ItemPic">
            <generatedKey column="pic_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_item_trace" schema="" domainObjectName="ItemTrace">
            <generatedKey column="trace_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_items" schema="" domainObjectName="Items">
            <generatedKey column="items_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_order" schema="" domainObjectName="Order">
            <generatedKey column="order_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_order_shipping" schema="" domainObjectName="OrderShipping">
            <generatedKey column="shipping_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_user" schema="" domainObjectName="User">
            <generatedKey column="user_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_user_address" schema="" domainObjectName="UserAddress">
            <generatedKey column="id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_user_comuser" schema="" domainObjectName="UserComuser">
            <generatedKey column="comuser_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_user_shop" schema="" domainObjectName="UserShop">
            <generatedKey column="shop_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_order_item" schema="" domainObjectName="OrderItem">
            <generatedKey column="id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="consult_item" schema="" domainObjectName="ConsultItem">
            <generatedKey column="consult_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="ds_admin" schema="" domainObjectName="Admin">
            <generatedKey column="admin_id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="expert" schema="" domainObjectName="Expert" >
            <generatedKey column="expert" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="trace_mng" schema="" domainObjectName="TraceMng">
            <generatedKey column="mng_id" sqlStatement="mysql" identity="true"/>
        </table>
    </context>
</generatorConfiguration>